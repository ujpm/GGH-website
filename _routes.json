{
  "version": 1,
  "include": ["/*"],
  "exclude": [],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "https://backendggh.vercel.app/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/grants.*",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/scholarships.*",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/resources.*",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/dashboard.*",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": ".*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
