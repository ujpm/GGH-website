{
  "build": {
    "command": "npm run build",
    "directory": "dist",
    "environment": {
      "NODE_VERSION": "18"
    }
  },
  "routes": [
    {
      "src": "/api/*",
      "dest": "https://ggh-backend-hzmhjleg9-uwizeyimana-jean-pierres-projects.vercel.app/api/$1"
    },
    {
      "src": "/*",
      "dest": "/index.html"
    }
  ]
}
